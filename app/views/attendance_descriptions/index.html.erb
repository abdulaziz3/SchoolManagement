<p id="notice"><%= notice %></p>

<h1>Listing Attendance Descriptions</h1>

<table>
  <thead>
    <tr>
      <th>Att status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendance_descriptions.each do |attendance_description| %>
      <tr>
        <td><%= attendance_description.att_status %></td>
        <td><%= link_to 'Show', attendance_description %></td>
        <td><%= link_to 'Edit', edit_attendance_description_path(attendance_description) %></td>
        <td><%= link_to 'Destroy', attendance_description, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attendance description', new_attendance_description_path %>
