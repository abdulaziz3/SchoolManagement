<h1 align = 'center'> User </h1></br>
<div align ="center">
  <% @users.each do |user| %>
  <ul class = "listing">
  	<div class = "row">
  		<div class = "well will_categories col-md-4 col-md-offset-4">
  			<div class = "product-title">
  				<strong>
  					<%= link_to "#{user.full_name}", user_path(user) %>
  				</strong>
  				</br>
  				<li><small><%= pluralize(user.subjects.count, "subject") %></small></li>
  			</div>
  		</div>
  	</div>
  </ul>
  <% end %>
</div>
